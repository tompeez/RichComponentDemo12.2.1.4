<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich" xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
    <jsp:directive.page contentType="text/html;charset=UTF-8"></jsp:directive.page>
    <f:view>
        <af:document title="Number Format Demo" binding="#{templateBindings.documentComponent}" theme="dark" id="d1">
            <f:facet name="metaContainer">
                <!-- following jsf best practice of wrapping jsp include in a subview naming container -->
                <f:subview id="meta">
                    <jsp:include page="/templates/pageTemplateDefs/tagDemoMetaContainer.jsff"/>
                </f:subview>
            </f:facet>
            <af:pageTemplate id="demo" viewId="#{templates.componentTemplate}">
                <f:attribute name="visualDesignName" value=""/>
                <f:attribute name="demoKind" value="feature"/>
                <!-- Remove to enable tag inspector -->
                <f:attribute name="customEditorPresent" value="true"/>
                <!-- Document Title -->
                <f:attribute name="documentTitle" value="Number Format"/>
                <!-- Demo Name -->
                <f:attribute name="tagName" value="Number Format"/>
                <f:facet name="center">
                    <af:panelGroupLayout layout="scroll" id="layout0">
                        <f:facet name="separator">
                            <af:separator id="s1"/>
                        </f:facet>
                        <!-- START FEATURE DEMO -->
                        <af:outputText inlineStyle="font-size:large; display:block;" value="Chart Number Format" id="ot1"/>
                        <af:panelGroupLayout layout="vertical" id="pgl2">
                            <af:selectOneChoice label="Select a Chart:" id="soc1" value="#{chartSample.chartType}"
                                                autoSubmit="true">
                                <af:selectItem label="barChart" value="barChart"/>
                                <af:selectItem label="bubbleChart" value="bubbleChart"/>
                                <af:selectItem label="funnelChart" value="funnelChart"/>
                                <af:selectItem label="lineChart" value="lineChart"/>
                                <af:selectItem label="pieChart" value="pieChart"/>
                            </af:selectOneChoice>
                            <af:panelGroupLayout layout="vertical" id="pgl3" partialTriggers="soc1 button">
                                <af:outputText value="Series 2 is assigned to Y2 Axis for bar and line charts. Note that the type has to be set to 'currency' for the currency symbol or code to be displayed."
                                               id="ot3"/>
                                <af:switcher facetName="#{chartSample.chartType}" defaultFacet="barChart">
                                    <f:facet name="barChart">
                                        <dvt:barChart id="c1" value="#{chartDataSource.defaultBarData}" var="row">
                                            <f:facet name="dataStamp">
                                                <dvt:chartDataItem series="#{row.series}" group="#{row.group}"
                                                                   value="#{row.series == 'Series 2'? row.value *10000 : row.value}"/>
                                            </f:facet>
                                            <dvt:chartLegend rendered="false"/>
                                            <f:facet name="seriesStamp">
                                                <af:group>
                                                    <dvt:chartSeriesStyle assignedToY2="true" series="Series 2"/>
                                                </af:group>
                                            </f:facet>
                                            <dvt:chartValueFormat type="y" scaling="#{chartSample.yFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.yFormat.type}"
                                                                  groupingUsed="#{chartSample.yFormat.groupingUsed}"
                                                                  pattern="#{chartSample.yFormat.pattern}"
                                                                  currencyCode="#{chartSample.yFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.yFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="y2" scaling="#{chartSample.y2Format.scaling}">
                                                <af:convertNumber type="#{chartSample.y2Format.type}"
                                                                  groupingUsed="#{chartSample.y2Format.groupingUsed}"
                                                                  pattern="#{chartSample.y2Format.pattern}"
                                                                  currencyCode="#{chartSample.y2Format.currencyCode}"
                                                                  currencySymbol="#{chartSample.y2Format.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartYAxis>
                                                <dvt:chartTickLabel scaling="#{chartSample.yLabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.yLabelFormat.type}"
                                                                      groupingUsed="#{chartSample.yLabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.yLabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.yLabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.yLabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartYAxis>
                                            <dvt:chartY2Axis>
                                                <dvt:chartTickLabel scaling="#{chartSample.y2LabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.y2LabelFormat.type}"
                                                                      groupingUsed="#{chartSample.y2LabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.y2LabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.y2LabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.y2LabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartY2Axis>
                                        </dvt:barChart>
                                    </f:facet>
                                    <f:facet name="bubbleChart">
                                        <dvt:bubbleChart value="#{chartDataSource.defaultBubbleData}" var="row">
                                            <f:facet name="dataStamp">
                                                <dvt:chartDataItem series="#{row.series}" group="#{row.group}"
                                                                   x="#{row.x}" y="#{row.y}" z="#{row.z}"
                                                                   label="#{row.z}"/>
                                            </f:facet>
                                            <dvt:chartLegend rendered="false"/>
                                            <dvt:chartValueFormat type="label"
                                                                  scaling="#{chartSample.labelFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.labelFormat.type}"
                                                                  groupingUsed="#{chartSample.labelFormat.groupingUsed}"
                                                                  pattern="#{chartSample.labelFormat.pattern}"
                                                                  currencyCode="#{chartSample.labelFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.labelFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="x" scaling="#{chartSample.xFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.xFormat.type}"
                                                                  groupingUsed="#{chartSample.xFormat.groupingUsed}"
                                                                  pattern="#{chartSample.xFormat.pattern}"
                                                                  currencyCode="#{chartSample.xFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.xFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="y" scaling="#{chartSample.yFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.yFormat.type}"
                                                                  groupingUsed="#{chartSample.yFormat.groupingUsed}"
                                                                  pattern="#{chartSample.yFormat.pattern}"
                                                                  currencyCode="#{chartSample.yFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.yFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="z" scaling="#{chartSample.zFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.zFormat.type}"
                                                                  groupingUsed="#{chartSample.zFormat.groupingUsed}"
                                                                  pattern="#{chartSample.zFormat.pattern}"
                                                                  currencyCode="#{chartSample.zFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.zFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartXAxis>
                                                <dvt:chartTickLabel scaling="#{chartSample.xLabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.xLabelFormat.type}"
                                                                      groupingUsed="#{chartSample.xLabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.xLabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.xLabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.xLabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartXAxis>
                                            <dvt:chartYAxis>
                                                <dvt:chartTickLabel scaling="#{chartSample.yLabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.yLabelFormat.type}"
                                                                      groupingUsed="#{chartSample.yLabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.yLabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.yLabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.yLabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartYAxis>
                                        </dvt:bubbleChart>
                                    </f:facet>
                                    <f:facet name="lineChart">
                                        <dvt:lineChart value="#{chartDataSource.yearlyTimeData}" var="row"
                                                       timeAxisType="enabled">
                                            <f:facet name="dataStamp">
                                                <dvt:chartDataItem series="#{row.series}" group="#{row.group}"
                                                                   value="#{row.series == 'Series 2'? row.value *10000 : row.value}"/>
                                            </f:facet>
                                            <f:facet name="seriesStamp">
                                                <af:group>
                                                    <dvt:chartSeriesStyle assignedToY2="true" series="Series 2"/>
