                                                                  groupingUsed="#{chartSample.xFormat.groupingUsed}"
                                                                  pattern="#{chartSample.xFormat.pattern}"
                                                                  currencyCode="#{chartSample.xFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.xFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="y" scaling="#{chartSample.yFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.yFormat.type}"
                                                                  groupingUsed="#{chartSample.yFormat.groupingUsed}"
                                                                  pattern="#{chartSample.yFormat.pattern}"
                                                                  currencyCode="#{chartSample.yFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.yFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="y2" scaling="#{chartSample.y2Format.scaling}">
                                                <af:convertNumber type="#{chartSample.y2Format.type}"
                                                                  groupingUsed="#{chartSample.y2Format.groupingUsed}"
                                                                  pattern="#{chartSample.y2Format.pattern}"
                                                                  currencyCode="#{chartSample.y2Format.currencyCode}"
                                                                  currencySymbol="#{chartSample.y2Format.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartXAxis>
                                                <dvt:chartTickLabel scaling="#{chartSample.xLabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.xLabelFormat.type}"
                                                                      groupingUsed="#{chartSample.xLabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.xLabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.xLabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.xLabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartXAxis>
                                            <dvt:chartYAxis>
                                                <dvt:chartTickLabel scaling="#{chartSample.yLabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.yLabelFormat.type}"
                                                                      groupingUsed="#{chartSample.yLabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.yLabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.yLabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.yLabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartYAxis>
                                            <dvt:chartY2Axis>
                                                <dvt:chartTickLabel scaling="#{chartSample.y2LabelFormat.scaling}">
                                                    <af:convertNumber type="#{chartSample.y2LabelFormat.type}"
                                                                      groupingUsed="#{chartSample.y2LabelFormat.groupingUsed}"
                                                                      pattern="#{chartSample.y2LabelFormat.pattern}"
                                                                      currencyCode="#{chartSample.y2LabelFormat.currencyCode}"
                                                                      currencySymbol="#{chartSample.y2LabelFormat.currencySymbol}"/>
                                                </dvt:chartTickLabel>
                                            </dvt:chartY2Axis>
                                        </dvt:lineChart>
                                    </f:facet>
                                    <f:facet name="pieChart">
                                        <dvt:pieChart value="#{chartDataSource.defaultPieData}" var="row">
                                            <dvt:pieDataItem label="#{row.series}" value="#{row.value}" sliceLabel="#{row.value}"/>
                                            <dvt:chartLegend rendered="false"/>
                                            <dvt:chartValueFormat type="label"
                                                                  scaling="#{chartSample.labelFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.labelFormat.type}"
                                                                  groupingUsed="#{chartSample.labelFormat.groupingUsed}"
                                                                  pattern="#{chartSample.labelFormat.pattern}"
                                                                  currencyCode="#{chartSample.labelFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.labelFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="value"
