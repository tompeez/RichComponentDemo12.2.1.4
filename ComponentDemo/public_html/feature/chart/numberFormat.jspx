                                                                  groupingUsed="#{chartSample.valueLabelFormat.groupingUsed}"
                                                                  pattern="#{chartSample.valueLabelFormat.pattern}"
                                                                  currencyCode="#{chartSample.valueLabelFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.valueLabelFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                        </dvt:pieChart>
                                    </f:facet>
                                    <f:facet name="funnelChart">
                                        <dvt:funnelChart value="#{chartDataSource.defaultPieData}"
                                                         orientation="horizontal" sliceGaps="1" var="row">
                                            <dvt:funnelDataItem label="#{row.series}" value="#{row.value}"
                                                                targetValue="100"/>
                                            <dvt:chartValueFormat type="value"
                                                                  scaling="#{chartSample.valueLabelFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.valueLabelFormat.type}"
                                                                  groupingUsed="#{chartSample.valueLabelFormat.groupingUsed}"
                                                                  pattern="#{chartSample.valueLabelFormat.pattern}"
                                                                  currencyCode="#{chartSample.valueLabelFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.valueLabelFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                            <dvt:chartValueFormat type="targetValue"
                                                                  scaling="#{chartSample.targetValueFormat.scaling}">
                                                <af:convertNumber type="#{chartSample.targetValueFormat.type}"
                                                                  groupingUsed="#{chartSample.targetValueFormat.groupingUsed}"
                                                                  pattern="#{chartSample.targetValueFormat.pattern}"
                                                                  currencyCode="#{chartSample.targetValueFormat.currencyCode}"
                                                                  currencySymbol="#{chartSample.targetValueFormat.currencySymbol}"/>
                                            </dvt:chartValueFormat>
                                        </dvt:funnelChart>
                                    </f:facet>
                                </af:switcher>
                            </af:panelGroupLayout>
                            <af:panelGridLayout id="pgl1" partialTriggers="soc1" dimensionsFrom="children">
                                <af:gridRow height="100%" id="gr1"
                                            rendered="#{chartSample.chartType=='bubbleChart'|| chartSample.chartType=='lineChart'}">
