<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">

<jsp:directive.page contentType="text/html;charset=utf-8"></jsp:directive.page>  
  <f:view><af:document title="Point Theme Bucketing by Value" binding="#{templateBindings.documentComponent}" theme="dark" id="d1"><f:facet name="metaContainer"><!-- following jsf best practice of wrapping jsp include in a subview naming container --><f:subview id="meta"><jsp:include page="/templates/pageTemplateDefs/tagDemoMetaContainer.jsff"/></f:subview></f:facet>
    <af:pageTemplate id="demo" viewId="#{templates.componentTemplate}">
      <f:attribute name="visualDesignName" value=""/>
      <f:attribute name="demoKind" value="feature"/><f:attribute name="tagDocPrefix" value="dvt"/>
      
      <!-- Document Title -->
      <f:attribute name="documentTitle" value="Point Theme Bucketing by Value"/>

      <!-- Tag Name -->
      <f:attribute name="tagName" value="mapPointTheme"/>    
      
      <f:facet name="center">
          <af:group id="g1">
            <!-- START TAG DEMO -->
            <af:panelStretchLayout id="psl1" topHeight="auto"
                                   bottomHeight="80px">
              <f:facet name="center">
                <dvt:map id="map1" binding="#{elocationBean.component}"
                         startingY="0.0" startingX="0.0" mapZoom="0"
                         mapServerConfigId="mapConfig1"
                         baseMapName="ELOCATION_MERCATOR.WORLD_MAP"
                         autoZoomThemeId="pointTheme">
                  <dvt:mapPointTheme id="pointTheme"
                                     value="#{geoMapBacking.pointModel}">
                    <!-- If the value of a point from the data model falls within the minValue - maxValue 
            range of a mapPointStyleItem, then that style item is used to display the point -->
                    <dvt:mapPointStyleItem shortLabel="&lt;500" minValue="0.0"
                                           maxValue="500.0"
                                           imageURL="/resources/images/geoMap/normalHouse.gif"
                                           hoverImageURL="/resources/images/geoMap/dollar.gif"
                                           selectedImageURL="/resources/images/geoMap/dollar.gif"/>
                    <dvt:mapPointStyleItem shortLabel=">500" minValue="500.0"
                                           imageURL="/resources/images/geoMap/mansion.gif"
                                           hoverImageURL="/resources/images/geoMap/dollar.gif"
                                           selectedImageURL="/resources/images/geoMap/dollar.gif"/>
                  </dvt:mapPointTheme>
                  <dvt:mapOverview initialShown="false"/>
                </dvt:map>
              </f:facet>
              <f:facet name="start"/>
              <f:facet name="end"/>
              <f:facet name="top">
                <dvt:mapToolbar mapId="map1" id="mt1"/>
              </f:facet>
            </af:panelStretchLayout>
            <!-- END TAG DEMO -->
          </af:group>
        </f:facet>
        <f:facet name="centerBottomCollapsible">
            <af:panelGroupLayout layout="scroll" id="pgl1">
              <!-- START SEE ALSO LINKS -->
              <af:outputText value="See also:" id="ot1"/>
              <af:panelList id="pl1">
                <af:goLink text="Map with Point Theme"
                           destination="/components/mapPointTheme.jspx"
                           id="gl1"/>
                <af:goLink text="Map with Geocoded Point Theme"
                           destination="/feature/geoMap/geoMapGeocoding.jspx"
                           id="gl2"/>
                <af:goLink text="Point Theme Bucketing by Category"
                           destination="/feature/geoMap/geoMapPointCategoryBucketing.jspx"
                           id="gl3"/>
                <af:goLink text="Map Theme Custom Click Behaviors"
                           destination="/feature/geoMap/geoMapPopupDialog.jspx"
                           id="gl4"/>
                <af:goLink text="Map Theme Custom Info Window"
                           destination="/feature/geoMap/geoMapCustomInfoWindow.jspx"
                           id="gl5"/>
                <af:goLink text="Custom Default Point Styles"
                           destination="/feature/geoMap/geoMapCustomDefaultPoint.jspx"
                           id="gl6"/>
                <af:goLink text="Custom Point Callback"
                           destination="/feature/geoMap/geoMapCustomPointCallback.jspx"
                           id="gl7"/>
              </af:panelList>
              <!-- END SEE ALSO LINKS -->
            </af:panelGroupLayout>
          </f:facet>
    </af:pageTemplate>
  </af:document></f:view>
</jsp:root>
