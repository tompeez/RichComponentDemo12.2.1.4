<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">

<jsp:directive.page contentType="text/html;charset=utf-8"></jsp:directive.page>  
  <f:view><af:document title="#{ganttFeatureLinks.resourceUtilizationGanttDoubleClickTitle}" binding="#{templateBindings.documentComponent}" theme="dark" id="d1"><f:facet name="metaContainer"><!-- following jsf best practice of wrapping jsp include in a subview naming container --><f:subview id="meta"><jsp:include page="/templates/pageTemplateDefs/tagDemoMetaContainer.jsff"/></f:subview></f:facet>
    <af:pageTemplate id="demo" viewId="#{templates.componentTemplate}">
      <f:attribute name="visualDesignName" value=""/>
      <f:attribute name="demoKind" value="feature"/>
      <f:attribute name="customEditorPresent" value="true"/>
      
      <!-- Document Title -->
      <f:attribute name="documentTitle" value="Resource Utilization Gantt Double-Click"/>
      
      <!-- Demo Name -->
      <f:attribute name="tagName" value="Resource Utilization Gantt Double-Click"/>
      
      <f:facet name="center">
        <af:panelGroupLayout layout="scroll" id="layout0">
          <f:facet name="separator">
            <af:separator id="s1"/>
          </f:facet>

        <!-- START FEATURE DEMO -->
        <dvt:resourceUtilizationGantt id="resourceUtilizationGanttDoubleClick"
                          startTime="2008-01-01" endTime="2008-01-31"
                          value="#{resourceUtilizationGanttDoubleClick.model}"
                          var="resourceObj" taskbarFormatManager="#{resourceUtilizationGanttDoubleClick.taskbarFormatManager}"
                          doubleClickListener="#{resourceUtilizationGanttDoubleClick.handleDoubleClick}"
                          metrics="#{resourceUtilizationGanttDoubleClick.metrics}"
                          summary="Resource Utilization Gantt Double-Click Demo">
          <f:facet name="major">
            <dvt:timeAxis scale="weeks" id="ta1"/>
          </f:facet>
          <f:facet name="minor">
            <dvt:timeAxis scale="days" id="ta2"/>
          </f:facet>
          <f:facet name="nodeStamp">
            <af:column headerText="Resource Name" id="c1">
              <af:outputText value="#{resourceObj.resourceName}" id="ot1"/>
            </af:column>
          </f:facet>
          <af:column headerText="Department" id="c2">
            <af:outputText value="#{resourceObj.department}" id="ot2"/>
          </af:column>
        </dvt:resourceUtilizationGantt>
        <!-- END FEATURE DEMO -->        
        </af:panelGroupLayout>
      </f:facet>
      <f:facet name="centerBottomCollapsible">  
          <f:subview id="links">
            <jsp:include page="/feature/gantt/ganttFeatureLinks.jspx" flush="true"/>
          </f:subview>
      </f:facet>   
    </af:pageTemplate>
  </af:document></f:view>
</jsp:root>
