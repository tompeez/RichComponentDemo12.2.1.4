<jsp:root xmlns="http://www.w3.org/1999/xhtml"
          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich" version="1.2">
  <jsp:directive.page contentType="text/html;charset=utf-8"/>
  <f:view>
    <af:document title="panelCollection with a detailStamp Facet" binding="#{templateBindings.documentComponent}"
                 smallIconSource="#{aboutBean.smallIconSource}"
                 largeIconSource="#{aboutBean.largeIconSource}" theme="dark" id="d1">
      <af:pageTemplate id="dmoTpl" viewId="#{templates.componentTemplate}">
        <f:attribute name="tagName" value="panelCollection"/>
        <f:attribute name="visualDesignName" value=""/>
        <f:attribute name="demoKind" value="component"/>
        <f:facet name="center">
          <af:panelStretchLayout topHeight="25" id="psl1" dimensionsFrom="auto">
            <f:facet name="top">
              <af:outputFormatted value="&lt;b>panelCollection with detailStamp Facet Demo&lt;/b>"
                                  id="of1"/>
            </f:facet>
            <f:facet name="center">
              <af:panelCollection binding="#{editor.component}" id="panelCollection">
                <f:facet name="menus">
                  <af:menu text="Actions" id="m1">
                    <af:commandMenuItem text="Add..." id="cmi1"/>
                    <af:commandMenuItem text="Create.." id="cmi2"/>
                    <af:commandMenuItem text="Update..." disabled="true"
                                        id="cmi3"/>
                    <af:commandMenuItem text="Copy" id="cmi4"/>
                    <!-- todo: add af:group -->
                    <af:commandMenuItem text="Delete" id="cmi5"/>
                    <af:commandMenuItem text="Remove" accelerator="control A"
                                        id="cmi6"/>
                    <af:commandMenuItem text="Preferences" id="cmi7"/>
                  </af:menu>
                </f:facet>
                <f:facet name="toolbar">
                  <af:toolbar id="t1">
                    <af:commandToolbarButton shortDesc="Create" icon="/images/new_ena.png"
                                             id="ctb1"/>
                    <af:commandToolbarButton shortDesc="Edit" icon="/images/update_ena.png"
                                             id="ctb2"/>
                    <af:commandToolbarButton shortDesc="Delete" icon="/images/delete_ena.png"
                                             id="ctb3"/>
                  </af:toolbar>
                </f:facet>
                <f:facet name="secondaryToolbar">
                  <af:menuBar>
                    <af:commandMenuItem immediate="true" text="TagGuide" action="guide"/>
                    <af:goMenuItem text="view page source" destination="#{facesContext.viewRoot.viewId}.source"/>
                  </af:menuBar>
                </f:facet>
                <f:facet name="statusbar">
                  <af:toolbar id="t3">
                    <af:outputText id="statusText"
                                   inlineStyle="margin-left:5px; white-space:nowrap"
                                   value="Custom Statusbar Message"/>
                  </af:toolbar>
                </f:facet>
                <af:table var="test1" value="#{tableTestData}" id="table" rowSelection="multiple"
                          columnSelection="multiple" summary="table data">
                  <f:facet name="detailStamp">
                    <af:panelFormLayout rows="4" labelWidth="33%" fieldWidth="67%"
                                        inlineStyle="width:400px" id="pfl1">
                      <af:inputText label="Name" value="#{test1.name}" required="true"
                                    id="it1"/>
                      <af:group id="g1">
                        <af:inputText label="Size" value="#{test1.size}"
                                      id="it2"/>
                        <af:inputText label="Date Modified" value="#{test1.inputDate}"
                                      id="it3"/>
                        <af:inputText label="Created by" id="it4"/>
                      </af:group>
                    </af:panelFormLayout>
                  </f:facet>
                  <af:column width="40" rowHeader="true" headerText="Row Number"
                             id="c1">
                    <af:outputText value="#{test1.no}" id="ot1"/>
                  </af:column>
                  <af:column headerText="Number" displayIndex="1" align="end"
                             id="c2">
                    <af:outputText value="#{test1.no}" id="ot2"/>
                  </af:column>
                  <af:column sortProperty="name" headerText="Name" sortable="true"
                             displayIndex="2" id="c3">
                    <af:panelGroupLayout id="pgl1">
                      <af:image source="#{test1.icon}" inlineStyle="width:16px; height:14px;"
                                shortDesc="icon" id="i1"/>
                      <af:outputText inlineStyle="margin-left:8px" value="#{test1.name}"
                                     id="ot3"/>
                    </af:panelGroupLayout>
                  </af:column>
                  <af:column headerText="Size" frozen="true" displayIndex="0" align="center"
                             id="c4">
                    <af:column headerText="Size In KB width some more text to make it wrap"
                               noWrap="false" align="end" id="c5">
                      <af:outputText value="#{test1.size}" id="ot4"/>
                    </af:column>
                    <af:column headerText="Date Modified" align="center"
                               id="c6">
                      <af:outputText value="#{test1.date}" id="ot5"/>
                    </af:column>
                  </af:column>
                  <af:column headerText="Parent Col5/6" align="center" id="c7">
                    <af:column headerText="Col5 (Sortable)" align="center" sortProperty="col5"
                               sortable="true" id="c8">
                      <af:outputText value="#{test1.col5}" id="ot6"/>
                    </af:column>
                    <af:column headerText="Col6" align="center" id="c9">
                      <af:outputText value="#{test1.col6}" id="ot7"/>
                    </af:column>
                  </af:column>
                  <af:column headerText="Parent Col7/8/9/10" align="center"
                             id="c10">
                    <af:column headerText="Parent Col7/8" align="center"
                               id="c11">
                      <af:column align="end" headerText="Col7 (Sortable)" noWrap="false"
                                 sortProperty="col7" sortable="true" id="c12">
                        <af:outputText value="#{test1.col7}" id="ot8"/>
                      </af:column>
                      <af:column headerText="Col8 (Sortable)" align="end" sortProperty="col8"
                                 sortable="true" id="c13">
                        <af:outputText value="#{test1.col8}" id="ot9"/>
                      </af:column>
                    </af:column>
                    <af:column headerText="Parent Col9/10" align="center"
                               id="c14">
                      <af:column headerText="Col9" align="center" id="c15">
                        <af:outputText value="#{test1.col9}" id="ot10"/>
                      </af:column>
                      <af:column headerText="Col10" align="center" id="c16">
                        <af:outputText value="#{test1.col10}" id="ot11"/>
                      </af:column>
                    </af:column>
                  </af:column>
                  <af:column headerText="Parent Col11/12" align="center"
                             id="c17">
                    <af:column headerText="Col11" align="end" noWrap="false"
                               id="c18">
                      <af:outputText value="#{test1.col11}" id="ot12"/>
                    </af:column>
                    <af:column headerText="Col12" align="end" id="c19">
                      <af:outputText value="#{test1.col12}" id="ot13"/>
                    </af:column>
                  </af:column>
                  <af:column headerText="Col13" id="c20">
                    <af:outputText value="#{test1.col13}" id="ot14"/>
                  </af:column>
                  <af:column headerText="Col14" id="c21">
                    <af:outputText value="#{test1.col14}" id="ot15"/>
                  </af:column>
                  <af:column headerText="Col15" align="end" id="c22">
                    <af:outputText value="#{test1.col15}" id="ot16"/>
                  </af:column>
                  <af:column headerText="Col16" align="end" id="c23">
                    <af:outputText value="#{test1.col16}" id="ot17"/>
                  </af:column>
                  <af:column headerText="Col17" id="c24">
                    <af:outputText value="#{test1.col17}" id="ot18"/>
                  </af:column>
                  <af:column headerText="Col18" id="c25">
                    <af:outputText value="#{test1.col18}" id="ot19"/>
                  </af:column>
                  <af:column headerText="Col19" align="end" id="c26">
                    <af:outputText value="#{test1.col19}" id="ot20"/>
                  </af:column>
                  <af:column headerText="Col20" align="end" id="c27">
                    <af:outputText value="#{test1.col20}" id="ot21"/>
                  </af:column>
                  <af:column headerText="Col21" id="c28">
                    <af:outputText value="#{test1.col21}" id="ot22"/>
                  </af:column>
                  <af:column headerText="Col22" id="c29">
                    <af:outputText value="#{test1.col22}" id="ot23"/>
                  </af:column>
                  <af:column headerText="Col23" align="end" id="c30">
                    <af:outputText value="#{test1.col23}" id="ot24"/>
                  </af:column>
                  <af:column headerText="Col24" align="end" id="c31">
                    <af:outputText value="#{test1.col24}" id="ot25"/>
                  </af:column>
                </af:table>
              </af:panelCollection>
            </f:facet>
          </af:panelStretchLayout>
        </f:facet>
      </af:pageTemplate>
    </af:document>
  </f:view>
</jsp:root>
