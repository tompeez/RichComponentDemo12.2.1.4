<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich" version="1.2">
  <jsp:directive.page contentType="text/html;charset=utf-8"/>
  <f:view>
    <af:document title="gridRow Demo" binding="#{templateBindings.documentComponent}"
                 smallIconSource="#{aboutBean.smallIconSource}"
                 largeIconSource="#{aboutBean.largeIconSource}" theme="dark" id="d1">
      <af:pageTemplate id="dmoTpl" viewId="#{templates.componentTemplate}">
        <f:attribute name="tagName" value="gridRow"/>
        <f:attribute name="visualDesignName" value=""/>
        <f:attribute name="demoKind" value="component"/>
        <f:attribute name="centerFooterHeight" value="2em"/>
        <f:attribute name="gmChrome" value="shown"/>
        <f:attribute name="gmStretchChildren" value="first"/>
        <f:facet name="center">
          <af:panelGridLayout id="grid1" inlineStyle="background-color:hotpink">
            <af:gridRow id="gr1" binding="#{editor.component}" marginTop="10px" marginBottom="10px" height="33%">
              <af:gridCell id="gc1" marginStart="10px" marginEnd="10px" width="50%" halign="stretch" valign="stretch"
                           inlineStyle="background-color:#C0504D;color:#8C3836">
                <af:outputText id="ot1" value="Cell 1"/>
              </af:gridCell>
              <af:gridCell id="gc2" marginStart="10px" marginEnd="10px" width="50%" halign="stretch" valign="stretch"
                           inlineStyle="background-color:#F79646;color:#B66D31">
                <af:outputText id="ot2" value="Cell 2"/>
              </af:gridCell>
            </af:gridRow>
            <af:gridRow id="gr2" marginBottom="10px" height="33%">
              <af:gridCell id="gc3" inlineStyle="background-color:#9BBB59;color:#71893F" halign="stretch" valign="stretch">
                <af:outputText id="ot3" value="Cell 3"/>
              </af:gridCell>
              <af:gridCell id="gc4" inlineStyle="background-color:#4F81BD;color:#3F5D8A" halign="stretch" valign="stretch">
                <af:outputText id="ot4" value="Cell 4"/>
              </af:gridCell>
            </af:gridRow>
            <af:gridRow id="gr3" marginBottom="10px" height="33%">
              <af:gridCell id="gc5" inlineStyle="background-color:#8064A2;color:#5C4776" halign="stretch" valign="stretch">
                <af:outputText id="ot5" value="Cell 5"/>
              </af:gridCell>
              <af:gridCell id="gc6" inlineStyle="background-color:#EFEFEF;color:#888888" halign="stretch" valign="stretch">
                <af:outputText id="ot6" value="Cell 6"/>
              </af:gridCell>
            </af:gridRow>
          </af:panelGridLayout>
        </f:facet>
      </af:pageTemplate>
    </af:document>
  </f:view>
</jsp:root>
