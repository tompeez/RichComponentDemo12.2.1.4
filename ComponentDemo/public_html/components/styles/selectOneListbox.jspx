<jsp:root xmlns="http://www.w3.org/1999/xhtml"
          xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=utf-8"/>
  <f:view>
    <af:document title="selectOneListbox Style Demo" binding="#{templateBindings.documentComponent}"
                 smallIconSource="#{aboutBean.smallIconSource}"
                 largeIconSource="#{aboutBean.largeIconSource}" theme="dark" id="d1">
      <af:pageTemplate id="dmoTpl" viewId="#{templates.componentTemplate}">
        <f:attribute name="tagName" value="selectOneListbox"/>
        <f:attribute name="visualDesignName" value=""/>
        <f:attribute name="demoKind" value="styles"/>
        <f:attribute name="editorSize" value="350"/>
        <f:facet name="center">
          <af:panelGroupLayout layout="scroll" id="pgl1">
            <f:facet name="separator">
              <af:spacer height="20" id="s1"/>
            </f:facet>
            <af:outputText styleClass="AFInstructionText"
                           value="This page demos the effect various contentStyle and inlineStyle parameters have on this component. If you cannot see an entire word in the box below (for example, in Firefox 3 and Safari, the width of the box is not automatically increased to account for the width of the scroll bar), try selecting 'Box-->Width-->width:250px;' under either inlineStyle or contentStyle."
                           id="ot1"/>
            <af:selectOneListbox labelAndAccessKey="D&amp;rink" helpTopicId="TOPICID_ALL"
                                 changed="true" binding="#{styleChanger.components['selectOneListbox1']}"
                                 id="sol1">
              <af:selectItem label="coffee" value="coffee" id="si1"/>
              <f:selectItem itemLabel="tea" itemValue="tea" id="si2"/>
              <af:separator id="s2"/>
              <af:selectItem label="milk" value="milk" id="si3"/>
              <af:selectItem label="beer" value="beer" disabled="true"
                             id="si4"/>
              <af:selectItem label="redbull" value="wings" id="si5"/>
            </af:selectOneListbox>
            <af:selectOneListbox label="ReadOnly:" readOnly="true" value="tea"
                                 binding="#{styleChanger.components['selectOneListbox2']}"
                                 id="sol2">
              <af:selectItem label="coffee" value="coffee" id="si6"/>
              <f:selectItem itemLabel="tea" itemValue="tea" id="si7"/>
              <af:selectItem label="milk" value="milk" id="si8"/>
            </af:selectOneListbox>
            <af:selectOneListbox label="Disabled:" disabled="true" value="milk"
                                binding="#{styleChanger.components['selectOneListbox3']}"
                                 id="sol3">
              <af:selectItem label="coffee" value="coffee" id="si9"/>
              <f:selectItem itemLabel="tea" itemValue="tea" id="si10"/>
              <af:selectItem label="milk" value="milk" id="si11"/>
            </af:selectOneListbox>
          </af:panelGroupLayout>
        </f:facet>
      </af:pageTemplate>
    </af:document>
  </f:view>
</jsp:root>
