<jsp:root xmlns="http://www.w3.org/1999/xhtml"
          xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=utf-8"/>
  <f:view>
    <af:document title="progressIndicator Style Demo" binding="#{templateBindings.documentComponent}"
                 smallIconSource="#{aboutBean.smallIconSource}"
                 largeIconSource="#{aboutBean.largeIconSource}" theme="dark" id="d1">
      <af:pageTemplate id="dmoTpl" viewId="#{templates.componentTemplate}">
        <f:attribute name="tagName" value="progressIndicator"/>
        <f:attribute name="visualDesignName" value=""/>
        <f:attribute name="demoKind" value="styles"/>
        <f:attribute name="editorSize" value="350"/>
        <f:facet name="center">
          <af:panelGroupLayout layout="scroll" id="pgl1">
            <f:facet name="separator">
              <af:spacer height="10" id="s1"/>
            </f:facet>
            <af:outputText styleClass="AFInstructionText"
                           value="This page demos the effect various contentStyle and inlineStyle parameters have on this component."
                           id="ot1"/>
            <af:outputText value="Indeterminate Progress Indicator, Running"
                           id="ot2"/>
            <af:progressIndicator value="#{demoProgressIndicator.alwaysRunningIndeterminateModel}"
                                  shortDesc="shortDesc"
                                  binding="#{styleChanger.components['progressIndicator1']}"
                                  id="pi1"/>
            <af:outputText value="Indeterminate Progress Indicator, Finished"
                           id="ot3"/>
            <af:progressIndicator value="#{demoProgressIndicator.finishedRunningIndeterminateModel}"
                                  binding="#{styleChanger.components['progressIndicator2']}"
                                  id="pi2"/>
            <af:outputText value="Determinate Progress Indicator (Determinate 50% Finished)"
                           id="ot4"/>
            <af:progressIndicator value="#{demoProgressIndicator.fiftyPercentDeterminateModel}"
                                  binding="#{styleChanger.components['progressIndicator3']}"
                                  id="pi3"/>
          </af:panelGroupLayout>
        </f:facet>
      </af:pageTemplate>
    </af:document>
  </f:view>
</jsp:root>
