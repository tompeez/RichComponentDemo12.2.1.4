<?xml version="1.0" encoding="utf-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
  version="2.0">
  <namespace>http://xmlns.oracle.com/adf/faces/rich/test</namespace>

  <tag>
    <tag-name>callFunction</tag-name>
    <behavior>
      <behavior-id>call-function</behavior-id>
    </behavior>
  </tag>

  <tag>
    <tag-name>panelEditable</tag-name>
    <component>
      <component-type>oracle.adf.TestPanelEditable</component-type>
      <renderer-type>oracle.adf.rich.test.Editable</renderer-type>
      <handler-class>oracle.adfinternal.view.faces.facelets.rich.RichComponentHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>panelResizeNotify</tag-name>
    <component>
      <component-type>oracle.adf.TestPanelResizeNotify</component-type>
      <renderer-type>oracle.adf.rich.test.ResizeNotify</renderer-type>
      <handler-class>oracle.adfinternal.view.faces.facelets.rich.RichComponentHandler</handler-class>
    </component>
  </tag>
  
  <tag>
    <tag-name>panelStackedLayout</tag-name>
    <component>
      <component-type>oracle.adf.TestPanelStackedLayout</component-type>
      <renderer-type>oracle.adf.rich.test.StackedLayout</renderer-type>
      <handler-class>oracle.adfinternal.view.faces.facelets.rich.RichComponentHandler</handler-class>
    </component>
  </tag>
  
  <tag>
    <tag-name>testGeometryInheritance</tag-name>
    <component>
      <component-type>oracle.adf.TestGeometryInheritance</component-type>
      <renderer-type>oracle.adf.rich.test.TestGeometryInheritance</renderer-type>
      <handler-class>oracle.adfinternal.view.faces.facelets.rich.RichComponentHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>testGeometryManagement</tag-name>
    <component>
      <component-type>oracle.adf.TestGeometryManagement</component-type>
      <renderer-type>oracle.adf.rich.test.TestGeometryManagement</renderer-type>
      <handler-class>oracle.adfinternal.view.faces.facelets.rich.RichComponentHandler</handler-class>
    </component>
  </tag>

</facelet-taglib>
