<profileconf xmlns="http://xmlns.oracle.com/adf/rich/profiling" 
  prestart="true" 
  logging="true">
  <!-- 
    Some basic checkpoints for timing startup/postback processing.
    To view data for these checkpoints, in web.xml set
    "oracle.adf.view.rich.LOGGER_LEVEL" to "INFO" and
    oracle.adf.view.rich.profiler.ENABLED to "true"
  -->


  <!-- Uncomment to enable startup profiling
  <checkpointOverride group="startup" value="true"/>
  -->
  
  <!-- Uncomment to enable postback profiling 
  <checkpointOverride group="postback" value="true"/>  
  <method classname="AdfDhtmlPage.prototype" methodname="_handleRichResponse"/>
  <method classname="AdfDhtmlPage.prototype" methodname="_handleRichResponseFragment"/>
  -->

  <!-- Uncomment to enable resize profiling
  <checkpointOverride group="resize" value="true"/>
  -->
  
  <!-- Uncomment to enable layout profiling
  <checkpointOverride group="layout" value="true"/>
  -->

</profileconf>
